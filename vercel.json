{
    "version": 2,
    "builds": [
      {
        "src": "index.js",  // ou o nome do seu arquivo principal
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
       {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }