{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "client",
            "use": "@vercel/react"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/client/$1"
        },
         {
            "src": "/",
            "dest": "/client/index.jsx"
        }
    ]
}